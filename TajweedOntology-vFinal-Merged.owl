        <rdfs:range rdf:resource="http://www.semantictajweed.com/ontology/CharacteristicsWithOpposite"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/isPartOf"/>
    


    <!-- http://www.semantictajweed.com/ontology/isPartOfChapter -->

