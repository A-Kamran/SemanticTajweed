    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/followedBy -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/followedBy"/>
    


    <!-- http://www.semantictajweed.com/ontology/hasArticulationPoint -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/hasArticulationPoint">
        <owl:inverseOf rdf:resource="http://www.semantictajweed.com/ontology/isArticulationPointof"/>
        <rdfs:domain rdf:resource="http://www.semantictajweed.com/ontology/Letter"/>
        <rdfs:range rdf:resource="http://www.semantictajweed.com/ontology/ArticulationPoint"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/hasCharacteristic -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/hasCharacteristic">
