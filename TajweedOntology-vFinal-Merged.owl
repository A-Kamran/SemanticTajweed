        <rdfs:range rdf:resource="http://www.semantictajweed.com/ontology/Verse"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/hasWord -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/hasWord">
        <rdfs:subPropertyOf rdf:resource="http://www.semantictajweed.com/ontology/hasPart"/>
        <owl:inverseOf rdf:resource="http://www.semantictajweed.com/ontology/isPartOfVerse"/>
        <rdfs:domain rdf:resource="http://www.semantictajweed.com/ontology/Verse"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/involvesAnatomicalUnit -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/involvesAnatomicalUnit">
        <rdfs:domain rdf:resource="http://www.semantictajweed.com/ontology/ArticulationPoint"/>
        <rdfs:range rdf:resource="http://www.semantictajweed.com/ontology/AnatomicalUnit"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/involvesArticulationArea -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/involvesArticulationArea">
        <owl:inverseOf rdf:resource="http://www.semantictajweed.com/ontology/isInvolvedInArticulationOf"/>
        <rdfs:range rdf:resource="http://www.semantictajweed.com/ontology/AreaOfArticulation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/involvesDiacritic -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/involvesDiacritic"/>
    


    <!-- http://www.semantictajweed.com/ontology/involvesLetter -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/involvesLetter">
