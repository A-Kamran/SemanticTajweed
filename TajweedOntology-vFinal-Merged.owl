        <owl:inverseOf rdf:resource="http://www.semantictajweed.com/ontology/occursAt"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/hasRuleOccurrence -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/hasRuleOccurrence">
