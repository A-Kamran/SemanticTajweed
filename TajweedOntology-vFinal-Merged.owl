        <rdfs:range rdf:resource="http://www.semantictajweed.com/ontology/LetterOccurrence"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/hasPart -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/hasPart"/>
    


    <!-- http://www.semantictajweed.com/ontology/hasRuleInstance -->

