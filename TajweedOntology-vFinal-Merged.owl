        <rdfs:range rdf:resource="http://www.semantictajweed.com/ontology/Letter"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/occursAt -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/occursAt"/>
    


    <!-- http://www.semantictajweed.com/ontology/partiallyInvolves -->

