        <term:creator>Amna Basharat</term:creator>
        <term:creator>Amna Binte Kamran</term:creator>
        <term:description>An ontology about Arabic Letters and their characteristics in the recitation of The Holy Quran. The ontology is annotated in both Arabic and English.</term:description>
        <term:license>CC BY-SA 4.0</term:license>
        <rdfs:isDefinedBy>tajweed:</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Tajweed Rules</rdfs:label>
        <rdfs:label xml:lang="ar">قواعد التجويد</rdfs:label>
    </owl:Ontology>
    


