        <rdfs:domain rdf:resource="http://www.semantictajweed.com/ontology/Letter"/>
        <rdfs:range rdf:resource="http://www.semantictajweed.com/ontology/BasicCharacteristic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/hasCharacteristicType -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/hasCharacteristicType">
        <owl:inverseOf rdf:resource="http://www.semantictajweed.com/ontology/hasCharacteristicType"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semantictajweed.com/ontology/BasicCharacteristic"/>
        <rdfs:domain rdf:resource="http://www.semantictajweed.com/ontology/TypeOfCharacteristic"/>
        <rdfs:range rdf:resource="http://www.semantictajweed.com/ontology/BasicCharacteristic"/>
        <rdfs:range rdf:resource="http://www.semantictajweed.com/ontology/TypeOfCharacteristic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semantictajweed.com/ontology/hasLetter -->

    <owl:ObjectProperty rdf:about="http://www.semantictajweed.com/ontology/hasLetter">
