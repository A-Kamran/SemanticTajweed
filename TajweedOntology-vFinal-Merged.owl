    <!-- http://www.semantictajweed.com/ontology/chapterIndex -->

    <owl:DatatypeProperty rdf:about="http://www.semantictajweed.com/ontology/chapterIndex"/>
    


    <!-- http://www.semantictajweed.com/ontology/hasAppliedTajweedDefinition -->

    <owl:DatatypeProperty rdf:about="http://www.semantictajweed.com/ontology/hasAppliedTajweedDefinition">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Definitions of the concepts or instances that are applied in practical learning of Tajweed</rdfs:comment>
    </owl:DatatypeProperty>
    


