    <!-- http://www.semantictajweed.com/ontology/AnatomicalUnit -->

    <owl:Class rdf:about="http://www.semantictajweed.com/ontology/AnatomicalUnit"/>
    


    <!-- http://www.semantictajweed.com/ontology/ApproximateArticulationPoint -->

    <owl:Class rdf:about="http://www.semantictajweed.com/ontology/ApproximateArticulationPoint">
        <rdfs:subClassOf rdf:resource="http://www.semantictajweed.com/ontology/ArticulationPoint"/>
    </owl:Class>
    


    <!-- http://www.semantictajweed.com/ontology/AreaOfArticulation -->

    <owl:Class rdf:about="http://www.semantictajweed.com/ontology/AreaOfArticulation">
        <rdfs:subClassOf rdf:resource="http://www.semantictajweed.com/ontology/AnatomicalUnit"/>
        <owl:disjointWith rdf:resource="http://www.semantictajweed.com/ontology/Characteristic"/>
        <owl:disjointWith rdf:resource="http://www.semantictajweed.com/ontology/Letter"/>
        <owl:disjointWith rdf:resource="http://www.semantictajweed.com/ontology/LexicalEntity"/>
    </owl:Class>
    


    <!-- http://www.semantictajweed.com/ontology/ArticulationPoint -->

    <owl:Class rdf:about="http://www.semantictajweed.com/ontology/ArticulationPoint">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semantictajweed.com/ontology/isArticulationPointof"/>
                <owl:someValuesFrom rdf:resource="http://www.semantictajweed.com/ontology/Letter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semantictajweed.com/ontology/isPartOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semantictajweed.com/ontology/AreaOfArticulation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.semantictajweed.com/ontology/Characteristic"/>
        <owl:disjointWith rdf:resource="http://www.semantictajweed.com/ontology/Letter"/>
        <owl:disjointWith rdf:resource="http://www.semantictajweed.com/ontology/LexicalEntity"/>
    </owl:Class>
    


    <!-- http://www.semantictajweed.com/ontology/BasicCharacteristic -->

    <owl:Class rdf:about="http://www.semantictajweed.com/ontology/BasicCharacteristic">
        <rdfs:subClassOf rdf:resource="http://www.semantictajweed.com/ontology/Characteristic"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semantictajweed.com/ontology/hasCharacteristicType"/>
                <owl:someValuesFrom rdf:resource="http://www.semantictajweed.com/ontology/TypeOfCharacteristic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.semantictajweed.com/ontology/ConditionalCharacteristic"/>
    </owl:Class>
    


    <!-- http://www.semantictajweed.com/ontology/BasicHarakaat -->

    <owl:Class rdf:about="http://www.semantictajweed.com/ontology/BasicHarakaat">
        <rdfs:subClassOf rdf:resource="http://www.semantictajweed.com/ontology/Diacritic"/>
    </owl:Class>
    


    <!-- http://www.semantictajweed.com/ontology/Chapter -->

    <owl:Class rdf:about="http://www.semantictajweed.com/ontology/Chapter"/>
    


    <!-- http://www.semantictajweed.com/ontology/Characteristic -->

    <owl:Class rdf:about="http://www.semantictajweed.com/ontology/Characteristic">
        <owl:disjointWith rdf:resource="http://www.semantictajweed.com/ontology/Letter"/>
    </owl:Class>
    


    <!-- http://www.semantictajweed.com/ontology/CharacteristicsWithOpposite -->

    <owl:Class rdf:about="http://www.semantictajweed.com/ontology/CharacteristicsWithOpposite">
        <rdfs:subClassOf rdf:resource="http://www.semantictajweed.com/ontology/BasicCharacteristic"/>
        <owl:disjointWith rdf:resource="http://www.semantictajweed.com/ontology/CharacteristicsWithoutOpposite"/>
    </owl:Class>
    


