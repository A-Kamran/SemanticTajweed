@prefix : <http://www.semantictajweed.com/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semantictajweed.com/ontology/> .

<http://www.semantictajweed.com/ontology/> rdf:type owl:Ontology ;
                                            owl:versionIRI <http://www.semantictajweed.com/ontology/1.0.0> ;
                                            <http://purl.org/dc/terms/creator> "Amna Basharat" ,
                                                                               "Amna Binte Kamran" ;
                                            <http://purl.org/dc/terms/description> "An ontology about Arabic Letters and their characteristics in the recitation of The Holy Quran. The ontology is annotated in both Arabic and English." ;
                                            <http://purl.org/dc/terms/license> "CC BY-SA 4.0" ;
                                            rdfs:isDefinedBy "tajweed:" ;
                                            rdfs:label "Tajweed Rules"@en ,
                                                       "قواعد التجويد"@ar .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/contributor
<http://purl.org/dc/terms/contributor> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#equivalentClass
owl:equivalentClass rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
<http://www.w3.org/2004/02/skos/core#altLabel> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semantictajweed.com/ontology/belongsToGroup
:belongsToGroup rdf:type owl:ObjectProperty ;
                rdfs:domain :Letter ;
                rdfs:range :GroupOfLetters .


###  http://www.semantictajweed.com/ontology/followedBy
:followedBy rdf:type owl:ObjectProperty .


###  http://www.semantictajweed.com/ontology/hasArticulationPoint
:hasArticulationPoint rdf:type owl:ObjectProperty ;
                      owl:inverseOf :isArticulationPointof ;
                      rdfs:domain :Letter ;
                      rdfs:range :ArticulationPoint .


###  http://www.semantictajweed.com/ontology/hasCharacteristic
:hasCharacteristic rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isCharacteristicOf ;
                   rdfs:domain :Letter ;
                   rdfs:range :BasicCharacteristic .


###  http://www.semantictajweed.com/ontology/hasCharacteristicType
:hasCharacteristicType rdf:type owl:ObjectProperty ;
                       owl:inverseOf :hasCharacteristicType ;
                       rdf:type owl:IrreflexiveProperty ;
                       rdfs:domain :BasicCharacteristic ,
                                   :TypeOfCharacteristic ;
                       rdfs:range :BasicCharacteristic ,
                                  :TypeOfCharacteristic .


###  http://www.semantictajweed.com/ontology/hasLetter
:hasLetter rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasPart ;
           rdf:type owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:domain :Word ;
           rdfs:range :Letter .


###  http://www.semantictajweed.com/ontology/hasLetterOccurrence
:hasLetterOccurrence rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasPart ;
                     owl:inverseOf :isPartOfWord ;
                     rdfs:domain :Word ;
                     rdfs:range :LetterOccurrence .


###  http://www.semantictajweed.com/ontology/hasPart
:hasPart rdf:type owl:ObjectProperty .


###  http://www.semantictajweed.com/ontology/hasRuleInstance
:hasRuleInstance rdf:type owl:ObjectProperty ;
                 owl:inverseOf :occursAt .


###  http://www.semantictajweed.com/ontology/hasRuleOccurrence
:hasRuleOccurrence rdf:type owl:ObjectProperty ;
                   owl:inverseOf :hasRuleType .


###  http://www.semantictajweed.com/ontology/hasRuleState
:hasRuleState rdf:type owl:ObjectProperty .


###  http://www.semantictajweed.com/ontology/hasRuleType
:hasRuleType rdf:type owl:ObjectProperty .


###  http://www.semantictajweed.com/ontology/hasVerse
:hasVerse rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasPart ;
          owl:inverseOf :isPartOfChapter ;
          rdfs:domain :Chapter ;
          rdfs:range :Verse .


###  http://www.semantictajweed.com/ontology/hasWord
:hasWord rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasPart ;
         owl:inverseOf :isPartOfVerse ;
         rdfs:domain :Verse .


###  http://www.semantictajweed.com/ontology/involvesAnatomicalUnit
:involvesAnatomicalUnit rdf:type owl:ObjectProperty ;
                        rdfs:domain :ArticulationPoint ;
                        rdfs:range :AnatomicalUnit .


###  http://www.semantictajweed.com/ontology/involvesArticulationArea
:involvesArticulationArea rdf:type owl:ObjectProperty ;
                          owl:inverseOf :isInvolvedInArticulationOf ;
                          rdfs:range :AreaOfArticulation .


###  http://www.semantictajweed.com/ontology/involvesDiacritic
:involvesDiacritic rdf:type owl:ObjectProperty .


###  http://www.semantictajweed.com/ontology/involvesLetter
:involvesLetter rdf:type owl:ObjectProperty ;
                rdfs:domain :LetterOccurrence ;
                rdfs:range :Letter .


###  http://www.semantictajweed.com/ontology/involvesPauseMarker
:involvesPauseMarker rdf:type owl:ObjectProperty .


###  http://www.semantictajweed.com/ontology/isArticulationPointof
:isArticulationPointof rdf:type owl:ObjectProperty ;
                       rdfs:domain :ArticulationPoint ;
                       rdfs:range :Letter .


###  http://www.semantictajweed.com/ontology/isCharacteristicOf
:isCharacteristicOf rdf:type owl:ObjectProperty ,
                             owl:AsymmetricProperty ;
                    rdfs:domain :BasicCharacteristic ;
                    rdfs:range :Letter .


###  http://www.semantictajweed.com/ontology/isCloseTo
:isCloseTo rdf:type owl:ObjectProperty ;
           rdfs:domain :Letter ;
           rdfs:range :Letter .


###  http://www.semantictajweed.com/ontology/isInvolvedInArticulationOf
:isInvolvedInArticulationOf rdf:type owl:ObjectProperty ;
                            rdfs:domain :AreaOfArticulation .


###  http://www.semantictajweed.com/ontology/isOppositeOf
:isOppositeOf rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ;
              rdfs:domain :CharacteristicsWithOpposite ;
              rdfs:range :CharacteristicsWithOpposite .


###  http://www.semantictajweed.com/ontology/isPartOf
:isPartOf rdf:type owl:ObjectProperty .


###  http://www.semantictajweed.com/ontology/isPartOfChapter
:isPartOfChapter rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :isPartOf .


###  http://www.semantictajweed.com/ontology/isPartOfVerse
:isPartOfVerse rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :isPartOf .


###  http://www.semantictajweed.com/ontology/isPartOfWord
:isPartOfWord rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isPartOf .


###  http://www.semantictajweed.com/ontology/isSimilarTo
:isSimilarTo rdf:type owl:ObjectProperty ;
             rdfs:domain :Letter ;
             rdfs:range :Letter .


###  http://www.semantictajweed.com/ontology/occursAt
:occursAt rdf:type owl:ObjectProperty .


###  http://www.semantictajweed.com/ontology/partiallyInvolves
:partiallyInvolves rdf:type owl:ObjectProperty ;
                   rdfs:domain :CharacteristicsWithOpposite ;
                   rdfs:range :CharacteristicsWithOpposite .


###  http://www.semantictajweed.com/ontology/precededBy
:precededBy rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semantictajweed.com/ontology/chapterIndex
:chapterIndex rdf:type owl:DatatypeProperty .


###  http://www.semantictajweed.com/ontology/hasAppliedTajweedDefinition
:hasAppliedTajweedDefinition rdf:type owl:DatatypeProperty ;
                             rdfs:domain owl:Thing ;
                             rdfs:range xsd:string ;
                             rdfs:comment "Definitions of the concepts or instances that are applied in practical learning of Tajweed" .


###  http://www.semantictajweed.com/ontology/hasDiacriticPosition
:hasDiacriticPosition rdf:type owl:DatatypeProperty ;
                      rdfs:range xsd:integer .


###  http://www.semantictajweed.com/ontology/hasExplaination
:hasExplaination rdf:type owl:DatatypeProperty ;
                 rdfs:domain owl:Thing ;
                 rdfs:range xsd:string .


###  http://www.semantictajweed.com/ontology/hasLetterPosition
:hasLetterPosition rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:integer .


###  http://www.semantictajweed.com/ontology/hasLinguisticDefinition
:hasLinguisticDefinition rdf:type owl:DatatypeProperty ;
                         rdfs:domain owl:Thing ;
                         rdfs:range xsd:string ;
                         rdfs:comment "Definition that is relating to language or lingustics" .


###  http://www.semantictajweed.com/ontology/hasSheikhAlJazareeReference
:hasSheikhAlJazareeReference rdf:type owl:DatatypeProperty ;
                             rdfs:domain owl:Thing ;
                             rdfs:range xsd:string ;
                             rdfs:comment """This is the matn (poem) of Muhammad bin Muhammad Bin Muhammad Bin 'Alee Bin Yuusuf Ibn Al Jazaree, who died in 833 Hijri, \"Al Muqaddimatu al Jazariyyah\" who has 109 verses, speaking of the rules of tajweed (art of Quranic recitation) and makhaarij Al huruuf (knowing the places from which the Arabic letters are pronounced). The poem is orignally in Arabic.

In our ontology we have annotated it in English as well.""" .


###  http://www.semantictajweed.com/ontology/revelationOrder
:revelationOrder rdf:type owl:DatatypeProperty .


###  http://www.semantictajweed.com/ontology/revelationPlace
:revelationPlace rdf:type owl:DatatypeProperty .


###  http://www.semantictajweed.com/ontology/verseCount
:verseCount rdf:type owl:DatatypeProperty .


###  http://www.semantictajweed.com/ontology/verseIndex
:verseIndex rdf:type owl:DatatypeProperty .


###  http://www.semantictajweed.com/ontology/verseText
:verseText rdf:type owl:DatatypeProperty .


###  http://www.semantictajweed.com/ontology/wordIndex
:wordIndex rdf:type owl:DatatypeProperty .


###  http://www.semantictajweed.com/ontology/wordRoot
:wordRoot rdf:type owl:DatatypeProperty .


###  http://www.semantictajweed.com/ontology/wordText
:wordText rdf:type owl:DatatypeProperty .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:DatatypeProperty .


###  http://www.w3.org/2000/01/rdf-schema#seeAlso
rdfs:seeAlso rdf:type owl:DatatypeProperty .


###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://www.semantictajweed.com/ontology/AnatomicalUnit
:AnatomicalUnit rdf:type owl:Class .


###  http://www.semantictajweed.com/ontology/ApproximateArticulationPoint
:ApproximateArticulationPoint rdf:type owl:Class ;
                              rdfs:subClassOf :ArticulationPoint .


###  http://www.semantictajweed.com/ontology/AreaOfArticulation
:AreaOfArticulation rdf:type owl:Class ;
                    rdfs:subClassOf :AnatomicalUnit ;
                    owl:disjointWith :Characteristic ,
                                     :Letter ,
                                     :LexicalEntity .


###  http://www.semantictajweed.com/ontology/ArticulationPoint
:ArticulationPoint rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty :isArticulationPointof ;
                                     owl:someValuesFrom :Letter
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isPartOf ;
                                     owl:someValuesFrom :AreaOfArticulation
                                   ] ;
                   owl:disjointWith :Characteristic ,
                                    :Letter ,
                                    :LexicalEntity .


###  http://www.semantictajweed.com/ontology/BasicCharacteristic
:BasicCharacteristic rdf:type owl:Class ;
                     rdfs:subClassOf :Characteristic ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasCharacteristicType ;
                                       owl:someValuesFrom :TypeOfCharacteristic
                                     ] ;
                     owl:disjointWith :ConditionalCharacteristic .


###  http://www.semantictajweed.com/ontology/BasicHarakaat
:BasicHarakaat rdf:type owl:Class ;
               rdfs:subClassOf :Diacritic .


###  http://www.semantictajweed.com/ontology/Chapter
:Chapter rdf:type owl:Class .


###  http://www.semantictajweed.com/ontology/Characteristic
:Characteristic rdf:type owl:Class ;
                owl:disjointWith :Letter .


###  http://www.semantictajweed.com/ontology/CharacteristicsWithOpposite
:CharacteristicsWithOpposite rdf:type owl:Class ;
                             rdfs:subClassOf :BasicCharacteristic ;
                             owl:disjointWith :CharacteristicsWithoutOpposite .


###  http://www.semantictajweed.com/ontology/CharacteristicsWithoutOpposite
:CharacteristicsWithoutOpposite rdf:type owl:Class ;
                                rdfs:subClassOf :BasicCharacteristic .


###  http://www.semantictajweed.com/ontology/CompleteIdghaamRule
:CompleteIdghaamRule rdf:type owl:Class ;
                     rdfs:subClassOf :IdghaamRule .


###  http://www.semantictajweed.com/ontology/CompleteIdghaamWithGhunnahLetter
:CompleteIdghaamWithGhunnahLetter rdf:type owl:Class ;
                                  rdfs:subClassOf :IdghaamWithGhunnahLetter .


###  http://www.semantictajweed.com/ontology/ConditionalCharacteristic
:ConditionalCharacteristic rdf:type owl:Class ;
                           rdfs:subClassOf :Characteristic .


###  http://www.semantictajweed.com/ontology/DammaDammatain
:DammaDammatain rdf:type owl:Class ;
                rdfs:subClassOf :Diacritic .


###  http://www.semantictajweed.com/ontology/Diacritic
:Diacritic rdf:type owl:Class ;
           rdfs:subClassOf :LexicalEntity .


###  http://www.semantictajweed.com/ontology/DisjointedLetter
:DisjointedLetter rdf:type owl:Class ;
                  rdfs:subClassOf :Letter .


###  http://www.semantictajweed.com/ontology/FathaDamma
:FathaDamma rdf:type owl:Class ;
            rdfs:subClassOf :Diacritic .


###  http://www.semantictajweed.com/ontology/FathaFathatain
:FathaFathatain rdf:type owl:Class ;
                rdfs:subClassOf :Diacritic .


###  http://www.semantictajweed.com/ontology/FathaKasra
:FathaKasra rdf:type owl:Class ;
            rdfs:subClassOf :Diacritic .


###  http://www.semantictajweed.com/ontology/FathainDammain
:FathainDammain rdf:type owl:Class ;
                rdfs:subClassOf :Diacritic .


###  http://www.semantictajweed.com/ontology/GhunnahLetter
:GhunnahLetter rdf:type owl:Class ;
               rdfs:subClassOf :Letter .


###  http://www.semantictajweed.com/ontology/GhunnahRule
:GhunnahRule rdf:type owl:Class ;
             rdfs:subClassOf :Rule .


###  http://www.semantictajweed.com/ontology/GroupOfLetters
:GroupOfLetters rdf:type owl:Class .


###  http://www.semantictajweed.com/ontology/HamzaLetter
:HamzaLetter rdf:type owl:Class ;
             rdfs:subClassOf :Letter .


###  http://www.semantictajweed.com/ontology/HamzatulwasalRule
:HamzatulwasalRule rdf:type owl:Class ;
                   rdfs:subClassOf :Rule .


###  http://www.semantictajweed.com/ontology/HaroofAshshafawiya
:HaroofAshshafawiya rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :Letter
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :hasArticulationPoint ;
                                                                 owl:hasValue :ArticulationPointofTwoLips
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ,
                                        [ rdf:type owl:Class ;
                                          owl:unionOf ( [ owl:intersectionOf ( :Letter
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty :hasArticulationPoint ;
                                                                                 owl:hasValue :ArticulationPointFaa
                                                                               ]
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasArticulationPoint ;
                                                          owl:hasValue :ArticuationPointWow
                                                        ]
                                                      )
                                        ] .


###  http://www.semantictajweed.com/ontology/HaroofeAlthalqeeyah
:HaroofeAlthalqeeyah rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Class ;
                                           owl:unionOf ( [ owl:intersectionOf ( :Letter
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty :hasArticulationPoint ;
                                                                                  owl:hasValue :ArticulationPointRaa
                                                                                ]
                                                                              ) ;
                                                           rdf:type owl:Class
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :hasArticulationPoint ;
                                                           owl:hasValue :ArticulationPointLaam
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :hasArticulationPoint ;
                                                           owl:hasValue :ArticulationPointNoon
                                                         ]
                                                       )
                                         ] ;
                     rdfs:subClassOf :Letter .


###  http://www.semantictajweed.com/ontology/HaroofeLahawiyyayn
:HaroofeLahawiyyayn rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:unionOf ( [ owl:intersectionOf ( :Letter
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty :hasArticulationPoint ;
                                                                                 owl:hasValue :ArticulationPointQaaf
                                                                               ]
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasArticulationPoint ;
                                                          owl:hasValue :ArticulationPointKaaf
                                                        ]
                                                      )
                                        ] ;
                    rdfs:subClassOf :Letter .


###  http://www.semantictajweed.com/ontology/HaroofeLathawiyya
:HaroofeLathawiyya rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :Letter
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasArticulationPoint ;
                                                                owl:hasValue :ArticulationPointTipofTongueandBottomEdges
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] .


###  http://www.semantictajweed.com/ontology/HaroofeMadeeya
:HaroofeMadeeya rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Letter
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasArticulationPoint ;
                                                             owl:hasValue :ArticulationPointE1
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :Letter .


###  http://www.semantictajweed.com/ontology/HaroofeNatiyya
:HaroofeNatiyya rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Letter
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasArticulationPoint ;
                                                             owl:hasValue :ArticulationPointofTopoftheTongue
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] .


###  http://www.semantictajweed.com/ontology/HaroofeSaafir
:HaroofeSaafir rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( :Letter
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hasArticulationPoint ;
                                                            owl:hasValue :ArticulationPointTipofTongue
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] .


###  http://www.semantictajweed.com/ontology/HaroofeShajariya
:HaroofeShajariya rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Letter
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasArticulationPoint ;
                                                               owl:hasValue :ArticulationPointofMiddlePartofTongue
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] .


